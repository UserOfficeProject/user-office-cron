{
  "name": "duo-cron-job",
  "version": "1.0.0",
  "description": "Node cron job service for ESS User Office project.",
  "main": "index.js",
  "repository": {
    "url": "https://gitlab.esss.lu.se/swap/duo-cron-job"
  },
  "scripts": {
    "dev": "env NODE_ENV=development node index.js",
    "build": "docker build -t dmsc/duo-cron-job .",
    "prod": "env NODE_ENV=production node index.js",
    "test": "jest --coverage",
    "lint": "eslint --fix . && echo Lint complete."
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "duo",
    "cron-job",
    "microservice",
    "ess",
    "user-office"
  ],
  "author": "SWAP",
  "license": "ISC",
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "eslint-plugin-jest": "^23.11.0",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "sinon": "^9.0.2"
  },
  "devDependencies": {
    "jest-fetch-mock": "^3.0.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
